snippet sh
#!/bin/bash
$0
endsnippet

snippet for
for ${1:variable} in ${2:file1 file2 file3};do
	${3:command1 on $variable}
done
endsnippet

snippet switch
case ${1:Expression} in
  ${2:pattern})
    ${3:statements}
    ;;$0
esac
endsnippet

snippet patt
  ${1:pattern})
    ${2:statements}
    ;;$0
endsnippet

