endsnippet

snippet br "border-right"
border-right: $1${2:px} ${3:solid} ${4:black};$0
endsnippet

snippet mb "margin-bottom"
margin-bottom: $1${2:px};$0
endsnippet

snippet pb "padding-bottom"
padding-bottom: $1${2:px};$0
endsnippet

snippet ml "margin-left"
margin-left: $1${2:px};$0
endsnippet

snippet pl "padding-left"
padding-left: $1${2:px};$0
endsnippet

snippet mr "margin-right"
margin-right: $1${2:px};$0
endsnippet

snippet pr "padding-right"
padding-right: $1${2:px};$0
endsnippet

snippet mg "margin: 0 0 0 0;"
margin: ${1:0}${2:px} ${3:0}${4:px} ${5:0}${6:px} ${7:0}${8:px};$0
endsnippet

snippet pd "padding: 0 0 0 0;"
padding: ${1:0}${2:px} ${3:0}${4:px} ${5:0}${6:px} ${7:0}${8:px};$0
endsnippet
